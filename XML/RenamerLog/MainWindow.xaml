<Window x:Class="RenamerLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RenamerLog"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="400" WindowStartupLocation="CenterScreen" Background="#FF2A3632">
    <Grid Margin="20" Background="#FF5E766E">
        <Button x:Name="openDialog" Content="Show Dialog" Width="80" Height="40"
                Click="openDialog_Click"/>
    </Grid>
</Window>
