<Window x:Class="RenameLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RenameLog"
        mc:Ignorable="d"
        Title="MainWindow" Height="322" Width="569" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="2" Grid.ColumnSpan="2" x:Name="buttonStart" Content="Start" Width="80" Margin="5" Click="buttonStart_Click"/>
        <TextBlock Grid.Row="0" Grid.Column="0" x:Name="textblockLeft" Text=""/>
        <TextBlock Grid.Row="0" Grid.Column="1" x:Name="textblockRight" Text=""/>
    </Grid>
</Window>
