<Window x:Class="LINQ_Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LINQ_Samples"
        mc:Ignorable="d"
        Title="MainWindow" Height="307" Width="534" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="26"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Background="LightBlue" VerticalAlignment="Top">
            <TextBlock Text="Any and Contains:" VerticalAlignment="Center" FontWeight="Bold"/>
            <Button x:Name="button001" Content="Start" Height="24" Width="60" Margin="10 0" Click="button001_Click"/>
            <TextBlock Text="List contains " VerticalAlignment="Center"/>
            <TextBox x:Name="textbox01" Text="xtra_004" VerticalAlignment="Center"/>
        </StackPanel>
        <TextBlock Grid.Row="2" x:Name="output" FontSize="14"/>
    </Grid>
</Window>
